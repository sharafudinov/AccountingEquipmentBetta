                                                  Stretch="Fill" 
                                                  SnapsToDevicePixels="True">
                                                    <Path.Data>
                                                                <PathGeometry>M7.5350111,4.500005 L8.5350113,4.500005 8.5350113,7.5000053 11.534998,7.5000053 11.534998,8.5000052 8.5350113,8.5000052 8.5350113,11.500005 7.5350111,11.500005 7.5350111,8.5000052 4.5349984,8.5000052 4.5349984,7.5000053 7.5350111,7.5000053 z M8.0360043,1 C4.1570021,1 1.0000002,4.1399999 1.0000002,7.9999998 1.0000002,11.86 4.1570021,15 8.0360043,15 11.915007,15 15.072009,11.86 15.072009,7.9999998 15.072009,4.1399999 11.915007,1 8.0360043,1 z M8.0360043,0 C12.467007,0 16.072009,3.5889997 16.072009,7.9999998 16.072009,12.411 12.467007,16 8.0360043,16 3.6050018,16 0,12.411 0,7.9999998 0,3.5889997 3.6050018,0 8.0360043,0 z</PathGeometry>
                                                    </Path.Data>
                                                </Path>
                                            </Button>
                                            <Slider x:Name="PART_ZoomSlider"
                                                Grid.Column="2"
                                                Width="150"
                                                Height="20"
                                                Maximum="500"
                                                Minimum="10"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                HorizontalContentAlignment="Center"
                                                Orientation="Horizontal"
                                                Focusable="False"
                                                IsMoveToPointEnabled="True"
                                                Value="{Binding ZoomFactor}"
                                                Style="{StaticResource WPFSliderStyle}">
                                            </Slider>
                                            <Button x:Name="PART_MinusZoomButton"
                                                Grid.Column="1"
                                                Width="24"
                                                Height="24"
                                                Focusable="False"
                                                HorizontalAlignment="Center"
                                                Style="{StaticResource WPFGlyphButtonStyle}">
                                                <Path x:Name="minuspath"
                                                  HorizontalAlignment="Center"
                                                  VerticalAlignment="Center" 
                                                  Width="16.072" 
                                                  Height="16"
                                                  Fill="{Binding Path=Foreground,RelativeSource={RelativeSource AncestorType={x:Type Button}}}"
                                                  Stretch="Fill" 
                                                  SnapsToDevicePixels="True">
                                                    <Path.Data>
                                                                <PathGeometry>M4.5349984,7.5000053 L11.534998,7.5000053 11.534998,8.5000052 4.5349984,8.5000052 z M8.0360043,1 C4.1570021,1 1.0000002,4.1399999 1.0000002,7.9999998 1.0000002,11.86 4.1570021,15 8.0360043,15 11.915007,15 15.072009,11.86 15.072009,7.9999998 15.072009,4.1399999 11.915007,1 8.0360043,1 z M8.0360043,0 C12.467007,0 16.072009,3.5889997 16.072009,7.9999998 16.072009,12.411 12.467007,16 8.0360043,16 3.6050018,16 0,12.411 0,7.9999998 0,3.5889997 3.6050018,0 8.0360043,0 z</PathGeometry>
                                                    </Path.Data>
                                                </Path>
                                            </Button>
                                        </Grid>
                                    </DockPanel>
                                </Border>
                            </Grid>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style BasedOn="{StaticResource SyncfusionPrintPreviewStyle}" TargetType="{x:Type PrintPreview_printing:PrintPreview}" />
    
</ResourceDictionary>
